{
  "name": "fio-registrations",
  "version": "0.1.0",
  "description": "FIO Account and Domain Registrations",
  "main": "index.js",
  "keywords": [
    "FIO"
  ],
  "license": "MIT",
  "scripts": {
    "start": "node server/index.js",
    "ngrok": "node ./server/ngrok",
    "serve": "nodemon --exec 'node server' & vue-cli-service serve",
    "build": "vue-cli-service build",
    "migrate": "sequelize-cli db:migrate",
    "sync": "node ./server/db/sync.js",
    "seed": "sequelize-cli db:seed:all",
    "postinstall": "npm run sync && npm run seed"
  },
  "dependencies": {
    "@fioprotocol/fiojs": "^0.5.1",
    "@fioprotocol/fiosdk": "^0.5.3",
    "@sendgrid/mail": "^6.5.1",
    "abort-controller": "^3.0.0",
    "aws-sdk": "^2.609.0",
    "bcryptjs": "^2.4.3",
    "bootstrap-vue": "^2.1.0",
    "clipboard-copy": "^3.1.0",
    "continuation-local-storage": "^3.2.1",
    "core-js": "^3.4.3",
    "create-hmac": "^1.1.7",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "isemail": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.2",
    "otplib": "^12.0.0",
    "pg": "^7.14.0",
    "pg-hstore": "^2.3.3",
    "qrcode": "^1.4.4",
    "reset-css": "^5.0.1",
    "s3-blob-store": "^4.1.0",
    "sendmail": "^1.6.1",
    "sequelize": "^5.21.3",
    "supports-color": "^7.1.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-node": "0.0.1-security",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bootstrap": "^4.4.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^4.0.2",
    "ngrok": "^3.2.7",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
    "sass-loader": "^8.0.0",
    "sequelize-cli": "^5.5.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "rules-dev-disable": {
    "vue/require-v-for-key": "warn",
    "vue/no-unused-components": "warn"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
